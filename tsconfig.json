{
	"extends": "@tsconfig/svelte/tsconfig.json",
	"include": ["src/**/*", "src/node_modules/**/*"],
	"exclude": ["node_modules/*", "__sapper__/*", "static/*"],
	"compilerOptions": {
		"allowJs": true,
		"esModuleInterop": true,
		"module": "esnext",
		"baseUrl": "./",
		"paths": {
			"src/*": ["src/*"]
		}
	}
}
